<?xml version="1.0" encoding="UTF-8" ?>
<ae:configurations
    xmlns:xi="http://www.w3.org/2001/XInclude"
    xmlns:ae="http://agavi.org/agavi/config/global/envelope/1.0"
    xmlns="http://berlinonline.de/schemas/honeybee/config/navigation/1.0"
    xmlns:env="http://berlinonline.de/schemas/honeybee/config/envelope/definition/1.0"
>
    <xi:include
        href="./includes/navigation.xml"
        xpointer="xmlns(ae=http://agavi.org/agavi/config/global/envelope/1.0) xpointer(/ae:configurations/*)"
    >
        <xi:fallback />
    </xi:include>

    <ae:configuration>
        <navigations default="main">
            <navigation name="main">
                <groups>
                    <group name="main_content">
                        <settings>
                            <setting name="enable_sticky_items">false</setting>
                        </settings>
                    </group>
                </groups>
            </navigation>
            
        </navigations>
    </ae:configuration>
</ae:configurations>
