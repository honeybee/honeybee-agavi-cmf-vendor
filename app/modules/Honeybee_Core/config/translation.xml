                            <ae:parameter name="breadcrumb_activity_1.label">Breadcrumb 1</ae:parameter>
                            <ae:parameter name="breadcrumb_activity_2.label">Breadcrumb 2</ae:parameter>
                            <ae:parameter name="breadcrumb_activity_1.description">Each breadcrumb item is connected to a simple activity</ae:parameter>
                            <ae:parameter name="breadcrumb_activity_2.description">Each breadcrumb item is connected to a simple activity</ae:parameter>
