                            <ae:parameter name="index.page_title">Honeybee</ae:parameter>
                            <ae:parameter name="theme_preview.page_title">Preview Theme</ae:parameter>
                            <ae:parameter name="system.status.page_title">Application Status</ae:parameter>
                            <ae:parameter name="system.error404.error404_success_view.page_title">404 Not Found</ae:parameter>
                        </ae:parameter>
                    </ae:parameter>
                    <ae:parameter name="activity">
                        <ae:parameter name="de">
                            <ae:parameter name="theme_preview.label">Vorschau Theme</ae:parameter>
                        </ae:parameter>
                        <ae:parameter name="en">
                            <ae:parameter name="theme_preview.label">Honeybee</ae:parameter>
                        </ae:parameter>
                    </ae:parameter>
                </message_translator>
            </translator>

            <translator domain="honeybee.core.theme_preview">
                <message_translator class="AgaviSimpleTranslator">
                    <ae:parameter name="activity">
                        <ae:parameter name="de">
                            <ae:parameter name="search.description">Search activity</ae:parameter>
                            <!-- Navigation activities -->
                            <ae:parameter name="sample_activity_1.label">Navigation item 1</ae:parameter>
                            <ae:parameter name="sample_activity_2.label">Navigation item 2</ae:parameter>
                            <ae:parameter name="sample_activity_3.label">Navigation item 3</ae:parameter>
