<?xml version="1.0" encoding="UTF-8"?>
<ae:configurations xmlns:ae="http://agavi.org/agavi/config/global/envelope/1.0" xmlns="http://agavi.org/agavi/config/parts/translation/1.0">
    <ae:configuration>
        <translators default_domain="application.ui">
            <!--
                Translations can be dependant on the workflow state of the root resource:

                    For FIELDS and ENTITIES is possible to define a per-state translation just appending
                    the workflow_state (see workflows.xml for a list of states) at the end of the
                    translation key (this is valid just for default translation keys defined into the
                    specific renderers).
                    If no state is appended then the state-less translation will be used regardless the
                    workflow_state.
            -->

            <!--
                {{ resource }} module's translations go here
            -->
            <translator domain="{{ vendor_prefix }}.{{ package_prefix }}.{{ resource_prefix }}">
                <message_translator class="AgaviSimpleTranslator">

                    <ae:parameter name="views">
                        <!--
                            Translation here will be used by default also for the browser title.
                        -->
                        <ae:parameter name="de">
                            <ae:parameter name="bulk.page_title"></ae:parameter>
                            <ae:parameter name="collection.page_title">{{ resource }}</ae:parameter>
                            <ae:parameter name="create.page_title">{{ resource }} hinzufügen</ae:parameter>
                            <ae:parameter name="embed.page_title"></ae:parameter>
                            <ae:parameter name="files.page_title"></ae:parameter>
                            <ae:parameter name="files.download.page_title"></ae:parameter>
                            <ae:parameter name="files.upload.page_title"></ae:parameter>
                            <ae:parameter name="hierarchy.page_title">{{ resource }} Hierarchie</ae:parameter>
                            <ae:parameter name="resource.page_title">{{ resource }} im Detail</ae:parameter>
                            <ae:parameter name="resource.embed.page_title"></ae:parameter>
                            <ae:parameter name="resource.history.page_title">{{ resource }} Geschichte</ae:parameter>
                            <ae:parameter name="resource.modify.page_title">{{ resource }} bearbeiten</ae:parameter>
                            <ae:parameter name="resource.task.page_title">{{ resource }} bearbeiten</ae:parameter>
                            <ae:parameter name="resource.task.proceed.page_title">{{ resource }}-Status ändern</ae:parameter>
                            <ae:parameter name="suggestions.page_title"></ae:parameter>
                            <ae:parameter name="summary.page_title"></ae:parameter>
                        </ae:parameter>
                        <ae:parameter name="en">
                            <ae:parameter name="bulk.page_title"></ae:parameter>
                            <ae:parameter name="collection.page_title">{{ resource }} list</ae:parameter>
                            <ae:parameter name="create.page_title">Create {{ resource }}</ae:parameter>
                            <ae:parameter name="embed.page_title"></ae:parameter>
                            <ae:parameter name="files.page_title"></ae:parameter>
                            <ae:parameter name="files.download.page_title"></ae:parameter>
                            <ae:parameter name="files.upload.page_title"></ae:parameter>
                            <ae:parameter name="hierarchy.page_title">{{ resource }} hierarchy</ae:parameter>
                            <ae:parameter name="resource.page_title">{{ resource }} detail</ae:parameter>
                            <ae:parameter name="resource.embed.page_title"></ae:parameter>
                            <ae:parameter name="resource.history.page_title">{{ resource }} history</ae:parameter>
                            <ae:parameter name="resource.modify.page_title">Modify {{ resource }}</ae:parameter>
                            <ae:parameter name="resource.task.page_title">Execute task on {{ resource }}</ae:parameter>
                            <ae:parameter name="resource.task.proceed.page_title">Change {{ resource }} status</ae:parameter>
                            <ae:parameter name="suggestions.page_title"></ae:parameter>
                            <ae:parameter name="summary.page_title"></ae:parameter>
                        </ae:parameter>
                    </ae:parameter>

                    <!--
                        View-template Tabs translations
                    -->
                    <ae:parameter name="tabs">
                        <ae:parameter name="de">
                        <!--
                            Example tab name translation:

                                <ae:parameter name="first_tab">{{ resource }} main section</ae:parameter>

                            Another translation domain can be passed as setting to the renderer, if needed.
                        -->
                        </ae:parameter>
                    </ae:parameter>

                    <!--
                        View-template Panels translations
                    -->
                    <ae:parameter name="panels">
                        <ae:parameter name="de">
                        <!--
                            Example panel translation:

                                <ae:parameter name="panel_name">Panel title</ae:parameter>
                                <ae:parameter name="panel_name.description">Detailed panel description</ae:parameter>

                            'panel_name' corresponds to the one defined into the view_template.xml

                            Another translation domain can be passed as setting to the renderer, if needed.
                        -->
                        </ae:parameter>
                    </ae:parameter>

                    <!--
                        {{ resource }} collection view's translations go here
                    -->
                    <ae:parameter name="collection">
                        <ae:parameter name="de">
                            <ae:parameter name="identifier">Identifier</ae:parameter>
                            <ae:parameter name="created_at">Erstellt</ae:parameter>
                            <ae:parameter name="modified_at">Zuletzt bearbeitet</ae:parameter>
                            <ae:parameter name="workflow_state">Status</ae:parameter>
                            <ae:parameter name="actions">Aktionen</ae:parameter>
                        </ae:parameter>
                        <ae:parameter name="en">
                            <ae:parameter name="identifier">Identifier</ae:parameter>
                            <ae:parameter name="created_at">Created at</ae:parameter>
                            <ae:parameter name="modified_at">Modified at</ae:parameter>
                            <ae:parameter name="workflow_state">Status</ae:parameter>
                            <ae:parameter name="actions">Actions</ae:parameter>
                        </ae:parameter>
                    </ae:parameter>

                    <!--
                        {{ resource }} fields translations go here
                    -->
                    <ae:parameter name="fields">
                        <ae:parameter name="de">
                            <!-- Default fields -->
                            <ae:parameter name="uuid.field_name">UUID</ae:parameter>
                            <ae:parameter name="workflow_state.field_name">Status</ae:parameter>
                            <ae:parameter name="created_at.field_name">Erstellt</ae:parameter>
                            <ae:parameter name="modified_at.field_name">Zuletzt bearbeitet</ae:parameter>
                            <ae:parameter name="identifier.field_name">Identifer</ae:parameter>
                            <ae:parameter name="revision.field_name">Revision</ae:parameter>
                            <ae:parameter name="version.field_name">Version</ae:parameter>
                            <ae:parameter name="language.field_name">Sprache</ae:parameter>
                            <ae:parameter name="add_images">Dateien hinzufügen</ae:parameter>
                            <ae:parameter name="text_list_remove_title">Löschen</ae:parameter>
                            <ae:parameter name="dragndrop_description">Einfach hier hereinziehen</ae:parameter>

                            <!-- Custom fields -->

                        </ae:parameter>
                        <ae:parameter name="en">
                            <!-- Default fields -->
                            <ae:parameter name="uuid.field_name">UUID</ae:parameter>
                            <ae:parameter name="workflow_state.field_name">Status</ae:parameter>
                            <ae:parameter name="created_at.field_name">Created at</ae:parameter>
                            <ae:parameter name="modified_at.field_name">Modified at</ae:parameter>
                            <ae:parameter name="identifier.field_name">Identifer</ae:parameter>
                            <ae:parameter name="revision.field_name">Revision</ae:parameter>
                            <ae:parameter name="version.field_name">Version</ae:parameter>
                            <ae:parameter name="language.field_name">Language</ae:parameter>
                            <ae:parameter name="add_images">Add Images</ae:parameter>
                            <ae:parameter name="text_list_remove_title">Remove</ae:parameter>
                            <ae:parameter name="dragndrop_description">Simply drag and drop here</ae:parameter>

                            <!-- Custom fields -->

                        </ae:parameter>
                    </ae:parameter>

                    <!--
                        {{ resource }} activities translations
                    -->
                    <ae:parameter name="activity">
                        <ae:parameter name="de">
                            <!--
                                Default views names (used by default for the navigation items)
                            -->
                            <ae:parameter name="collection.label">{{ resource }} Liste</ae:parameter>
                            <ae:parameter name="collection.description">{{ resource }} Liste</ae:parameter>
                            <ae:parameter name="resource.label">{{ resource }} Detail</ae:parameter>
                            <ae:parameter name="resource.description">{{ resource }} im Detail</ae:parameter>
                            <!--
                                Default workflow events names
                            -->
                            <ae:parameter name="edit.label">Bearbeiten</ae:parameter>
                            <ae:parameter name="edit.description">{{ resource }} bearbeiten</ae:parameter>
                            <ae:parameter name="delete.label">Löschen</ae:parameter>
                            <ae:parameter name="delete.description">{{ resource }} löschen</ae:parameter>
                            <ae:parameter name="promote.label">Aktivieren</ae:parameter>
                            <ae:parameter name="promote.description">{{ resource }} aktivieren</ae:parameter>
                            <ae:parameter name="demote.label">Deaktivieren</ae:parameter>
                            <ae:parameter name="demote.description">{{ resource }} deaktivieren</ae:parameter>
                            <!--
                                Default readonly activities
                            -->
                            <ae:parameter name="view_resource.label">Anschauen</ae:parameter>
                            <ae:parameter name="view_resource.description">{{ resource }} anschauen</ae:parameter>
                            <ae:parameter name="resource_history.label">Historie</ae:parameter>
                            <ae:parameter name="resource_history.description">{{ resource }} historie</ae:parameter>
                            <ae:parameter name="resource_hierarchy.label">Baumansicht</ae:parameter>
                            <ae:parameter name="resource_hierarchy.description">{{ resource }} baumansicht</ae:parameter>
                            <!--
                                Other activities translations
                            -->
                            <ae:parameter name="search.description">Suchen</ae:parameter>
                            <ae:parameter name="search.label">Suche</ae:parameter>
                            <ae:parameter name="create_resource.label">{{ resource }} hinzufügen</ae:parameter>
                            <ae:parameter name="create_resource.description">{{ resource }} erstellen</ae:parameter>
                            <ae:parameter name="save_resource.label">Speichern</ae:parameter>
                            <ae:parameter name="save_resource.description">Speichern</ae:parameter>
                            <ae:parameter name="save_resource_and_continue.label">Speichern und Neuen anlegen</ae:parameter>
                            <ae:parameter name="save_resource_and_continue.description">Speichern und Neuen anlegen</ae:parameter>
                            <ae:parameter name="back_to_collection.label">Zurück zur Liste</ae:parameter>
                            <ae:parameter name="back_to_collection.description">Zurück zur Liste</ae:parameter>
                            <ae:parameter name="sort_by_workflow_state.description">Aufsteigend nach Status sortiert</ae:parameter>
                            <ae:parameter name="sort_by_workflow_state.label">Status</ae:parameter>
                            <ae:parameter name="sort_by_modified_at.description">Aufsteigend nach Zuletzt bearbeitet sortiert</ae:parameter>
                            <ae:parameter name="sort_by_modified_at.label">Zuletzt bearbeitet</ae:parameter>
                            <ae:parameter name="prev_revision.label">Vorherige Version</ae:parameter>
                            <ae:parameter name="next_revision.label">Nächste Version</ae:parameter>
                        </ae:parameter>
                        <ae:parameter name="en">
                            <!--
                                Default views names (used by default for the navigation items)
                            -->
                            <ae:parameter name="collection.label">{{ resource }} List</ae:parameter>
                            <ae:parameter name="collection.description">List of {{ resource }}</ae:parameter>
                            <ae:parameter name="resource.label">{{ resource }} Detail</ae:parameter>
                            <ae:parameter name="resource.description">View detail of the {{ resource }}</ae:parameter>
                            <!--
                                Default workflow events names
                            -->
                            <ae:parameter name="edit.label">Edit</ae:parameter>
                            <ae:parameter name="edit.description">Edit the {{ resource }}</ae:parameter>
                            <ae:parameter name="delete.label">Delete</ae:parameter>
                            <ae:parameter name="delete.description">Delete the {{ resource }}</ae:parameter>
                            <ae:parameter name="promote.label">Promote</ae:parameter>
                            <ae:parameter name="promote.description">Promote the {{ resource }}</ae:parameter>
                            <ae:parameter name="demote.label">Demote</ae:parameter>
                            <ae:parameter name="demote.description">Demote the {{ resource }}</ae:parameter>
                            <!--
                                Default readonly activities
                            -->
                            <ae:parameter name="view_resource.label">View</ae:parameter>
                            <ae:parameter name="view_resource.description">View the {{ resource }}</ae:parameter>
                            <ae:parameter name="resource_history.label">History</ae:parameter>
                            <ae:parameter name="resource_history.description">Event history of the {{ resource }}</ae:parameter>
                            <ae:parameter name="resource_hierarchy.label">Hierarchy</ae:parameter>
                            <ae:parameter name="resource_hierarchy.description">{{ resource }} hierarchy</ae:parameter>
                            <!--
                                Other activities translations
                            -->
                            <ae:parameter name="search.description">Search by term</ae:parameter>
                            <ae:parameter name="search.label">Search</ae:parameter>
                            <ae:parameter name="create_resource.label">Create {{ resource }}</ae:parameter>
                            <ae:parameter name="create_resource.description">Create a new {{ resource }}</ae:parameter>
                            <ae:parameter name="save_resource.label">Save</ae:parameter>
                            <ae:parameter name="save_resource.description">Save the {{ resource }}</ae:parameter>
                            <ae:parameter name="save_resource_and_continue.label">Save and continue</ae:parameter>
                            <ae:parameter name="save_resource_and_continue.description">Save the {{ resource }} and continue</ae:parameter>
                            <ae:parameter name="back_to_collection.label">Back to collection</ae:parameter>
                            <ae:parameter name="back_to_collection.description">Back to the collection</ae:parameter>
                            <ae:parameter name="sort_by_workflow_state.label">Status</ae:parameter>
                            <ae:parameter name="sort_by_workflow_state.description">Sort by status</ae:parameter>
                            <ae:parameter name="sort_by_modified_at.description">Sort by modification date</ae:parameter>
                            <ae:parameter name="sort_by_modified_at.label">Last modified</ae:parameter>
                            <ae:parameter name="prev_revision.label">Previous version</ae:parameter>
                            <ae:parameter name="next_revision.label">Next version</ae:parameter>
                        </ae:parameter>
                    </ae:parameter>

                    <ae:parameter name="errors">
                        <ae:parameter name="de">
                            <ae:parameter name="The given {{ resource_prefix }}(identifier) is invalid.">Der angegebene {{ resource_prefix }}-Identifier ist nicht bekannt.</ae:parameter>
                            <ae:parameter name="file_upload.generic_error">Datei ist nicht akzeptabel.</ae:parameter>
                            <ae:parameter name="file_upload.upload_failed">Hochladen der Datei ist fehlgeschlagen.</ae:parameter>
                            <ae:parameter name="file_upload.min_size">Datei muss mindestens %min_size$s Bytes groß sein.</ae:parameter>
                            <ae:parameter name="file_upload.max_size">Datei überschreitet maximale Größe von %max_size$s Byte.</ae:parameter>
                            <ae:parameter name="file_upload.extension">Es werden nur Dateien mit folgenden Endungen akzeptiert: %extension$s.</ae:parameter>
                            <ae:parameter name="file_upload.mime_type">Dateityp falsch erkannt oder ist nicht erlaubt.</ae:parameter>
                            <ae:parameter name="file_upload.no_image">Hochgeladene Datei ist keine Bilddatei.</ae:parameter>
                            <ae:parameter name="file_upload.min_width">Minimal erlaubte Breite von %min_width$spx unterschritten.</ae:parameter>
                            <ae:parameter name="file_upload.max_width">Maximal erlaubte Breite von %max_width$spx überschritten.</ae:parameter>
                            <ae:parameter name="file_upload.min_height">Minimal erlaubte Höhe von %min_height$spx unterschritten.</ae:parameter>
                            <ae:parameter name="file_upload.max_height">Maximal erlaubte Höhe von %max_height$spx überschritten.</ae:parameter>
                            <ae:parameter name="file_upload.format">Als Bilddatei sind nur folgende Format akzeptabel: %format$s.</ae:parameter>
                        </ae:parameter>
                        <ae:parameter name="en">
                            <ae:parameter name="file_upload.generic_error">File is not acceptable.</ae:parameter>
                            <ae:parameter name="file_upload.upload_failed">Upload of file failed.</ae:parameter>
                            <ae:parameter name="file_upload.min_size">File must be at least %min_size$s bytes.</ae:parameter>
                            <ae:parameter name="file_upload.max_size">File is larger than %max_size$s bytey.</ae:parameter>
                            <ae:parameter name="file_upload.extension">Only allowed file extensions are: %extension$s.</ae:parameter>
                            <ae:parameter name="file_upload.mime_type">File type not recognized or not acceptable.</ae:parameter>
                            <ae:parameter name="file_upload.no_image">Uploaded file is not an image.</ae:parameter>
                            <ae:parameter name="file_upload.min_width">Minimum necessary width is %min_width$spx.</ae:parameter>
                            <ae:parameter name="file_upload.max_width">Maximum width of %max_width$spx exceeded.</ae:parameter>
                            <ae:parameter name="file_upload.min_height">Minimum height of %min_height$spx needed.</ae:parameter>
                            <ae:parameter name="file_upload.max_height">Maximum height of %max_height$spx allowed.</ae:parameter>
                            <ae:parameter name="file_upload.format">Given image file format not acceptable. Allowed: %format$s.</ae:parameter>
                        </ae:parameter>
                    </ae:parameter>
                </message_translator>
            </translator>

            <!--
                Resource related translations for the generic translation domain
            -->
            <translator domain="application">
                <message_translator class="AgaviSimpleTranslator">
                    <ae:parameter name="navigations">
                        <ae:parameter name="de">
                            <ae:parameter name="main.{{ vendor_prefix }}.{{ package_prefix }}.{{ resource_prefix }}">{{ vendor }}-{{ package }}-{{ resource }}</ae:parameter>
                        </ae:parameter>
                        <ae:parameter name="en">
                            <ae:parameter name="main.{{ vendor_prefix }}.{{ package_prefix }}.{{ resource_prefix }}">{{ vendor }}-{{ package }}-{{ resource }}</ae:parameter>
                        </ae:parameter>
                    </ae:parameter>
                </message_translator>
            </translator>

            <!--
                Others translation domains
            -->
            <translator domain="{{ vendor_prefix }}.{{ package_prefix }}.{{ resource_prefix }}">

                <message_translator class="AgaviSimpleTranslator">
                    <!--
                        {{ resource }}/Embed action's translations go here
                    -->
                    <ae:parameter name="embed">
                        <ae:parameter name="de">
                        </ae:parameter>
                        <ae:parameter name="en">
                        </ae:parameter>
                    </ae:parameter>

                    <!--
                        View-template Groups translations
                    -->
                    <ae:parameter name="groups">
                        <ae:parameter name="de">
                        </ae:parameter>
                        <ae:parameter name="en">
                        </ae:parameter>
                    </ae:parameter>
                </message_translator>
            </translator>
        </translators>
    </ae:configuration>
</ae:configurations>
